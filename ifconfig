config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option netmask '255.255.255.0'
	option ipaddr '10.42.0.2'
	option ldinstance '1'
	option lipinstance '1'
	option bridge_instance '1'
	option ifname 'eth6 eth5 eth3 eth4'

config interface 'wan'
	option type 'anywan'
	option proto 'dhcp'
	option hostname 'Inteno_82C2'
	option vendorid 'EG300A-WU21UDAC-INTENO'
	option ifname 'eth2.1'
	option reqopts '43 66 67 128 132 133 224'
	option conipinstance '1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'
	option reqaddress 'try'
	option reqprefix 'auto'


